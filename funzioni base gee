////////////////funzioni base di JAVA per google earth engine/////////////////

//variabile integer
var variable =1;


//variabile stringa
var string = 'stringa';

//stampo le variabili 
print (variable, string);


//variabile lista 
var list = [2, 6.7, 19, 34];
print (list);

//variabile dizionario
var dict = {
  'a': 'hello',
  'b': 8,
  'c': 'what',
};



//stampo il mio dizionario
print (dict)

//stampo con una funzione - dentro le parentesi ci sono gli argomenti
print (list, dict, string, variable)


